<block>
  <name>Cross Correlation No-Freq</name>
  <key>eecs_xcor2</key>
  <category>EECS</category>
  <import>import eecs</import>
  <make>eecs.xcor2($nSamples)</make>
  <callback>set_nSamples($nSamples)</callback>
  <param>
    <name>Nsamples</name>
    <key>nSamples</key>
    <value>2**8</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <nports>2</nports>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <nports>3</nports>
  </source>
</block>
